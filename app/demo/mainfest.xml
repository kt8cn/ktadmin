<?xml version="1.0" encoding="utf-8"?>
<manifest versionCode="">
	<application>
		<code>demo</code><!-- 应用标识 必须和文件夹名称一致 -->
		<name>狂团demo应用</name><!-- 应用名称-->
		<date>2022-09-06</date><!-- 日期 -->
		<version>1.0.0</version><!-- 版本 -->
		<description>这是一个demo示例应用</description><!-- 描述 -->
		<author>狂团</author><!-- 作者 -->
		<adminindex>/demo/admin/index</adminindex><!-- 应用的管理后台配置入口路径，不带域名，没有留空-->
		<userindex>/demo/user/index</userindex><!-- 应用的用户后台访问路径，不带域名 -->
		<target>1</target><!-- 首页打开方式1：当前窗口 2新窗口 -->
		<logo>https://kt8logo.oss-cn-beijing.aliyuncs.com/app.png</logo><!-- logo -->
		<type>1</type><!--类型：1主应用 2插件 3工具 4模板; 注: 插件类型时, 文件夹名必须在主应用后面拼接 plugin, 如demo_plugin_demo  -->
	</application>
</manifest>